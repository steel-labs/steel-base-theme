{
  "name": "steel_theme",
  "version": "1.0.0",
  "description": "A starting theme based on SASS, Bower, Gulp and a fancy state of mind",
  "main": "gulpfile.js",
  "scripts": {
    "start": "node ./node_modules/webpack/bin/webpack.js --progress --watch",
    "deploy": "node ./node_modules/webpack/bin/webpack.js --progress",
    "deploy-production": "node ./node_modules/webpack/bin/webpack.js -p --config webpack.production.config.js"
  },
  "devDependencies": {
    "babel-core": "~6.24.0",
    "babel-loader": "~6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "~6.24.0",
    "css-loader": "^0.27.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "~0.10.1",
    "node-sass": "~4.5.0",
    "run-sequence": "^1.2.2",
    "sass-loader": "~6.0.3",
    "style-loader": "~0.14.1",
    "url-loader": "~0.5.8",
    "webpack": "~2.2.1"
  },
  "dependencies": {
    "babel-preset-stage-1": "~6.22.0",
    "bourbon": "~4.2.7",
    "breakpoint-sass": "~2.7.0",
    "jquery": "~2.2.4",
    "js-cookie": "^2.1.2",
    "lodash": "~4.15.0",
    "susy": "~2.2.12"
  },
  "author": "Raffaele Abramini (raffaele.abramini@steellondon.com)",
  "homepage": "http://steellondon.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/steel-labs/steel-base-theme.git"
  },
  "license": "MIT"
}
